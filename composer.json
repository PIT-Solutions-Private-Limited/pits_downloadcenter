{
  "name": "pits/downloadcenter",
  "type": "typo3-cms-extension",
  "description": "Download Center for TYPO3 (FAL files can be shown in frontend with several filters)",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "homepage": "http://pitsolutions.ch/",
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "PITS\\PitsDownloadcenter\\": "Classes"
    }
  },
  "replace": {
        "pits_downloadcenter": "self.version",
        "typo3-ter/pits-download-center": "self.version"
  },
  "extra": {
        "typo3/cms": {
            "extension-key": "pits_downloadcenter"
        }
    }
}
